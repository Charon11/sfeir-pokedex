{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/assets/**/*.*"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
        "https://pokeapi.co/api/v2/pokemon/pikachu/",
        "https://pokeapi.co/api/v2/pokemon/charmander/"
      ],
      "cacheConfig": {
        "maxSize": 30,
        "maxAge": "7d",
        "strategy": "performance"
      }
    },
    {
      "name": "api-freshness-exclude",
      "urls": [
        "https://pokeapi.co/api/v2/pokemon/bulbasaur/"
      ],
      "cacheConfig": {
        "maxSize": 30,
        "maxAge": "0u",
        "strategy": "freshness"
      }
    },
    {
      "name": "api-performance-timeout",
      "urls": [
        "https://pokeapi.co/api/v2/pokemon/squirtle/"
      ],
      "cacheConfig": {
        "timeout": "10u",
        "maxSize": 30,
        "maxAge": "7d",
        "strategy": "freshness"
      }
    },
    {
      "name": "api-freshness",
      "urls": [
        "https://pokeapi.co/api/v2/pokemon",
        "https://pokeapi.co/api/v2/**/**"
      ],
      "cacheConfig": {
        "timeout": "500u",
        "maxSize": 30,
        "maxAge": "7d",
        "strategy": "freshness"
      }
    }
  ]
}
